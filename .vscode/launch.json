	"version": "0.2.0",
	"configurations": [
		{
			"type": "lldb",
			"request": "launch",
			"name": "Debug executable 'mysti-client'",
			"cargo": {
				"args": [
					"build",
					"--bin=mysti-client",
					"--package=client"
				],
				"filter": {
					"name": "mysti-client",
					"kind": "bin"
				}
			},
			"args": [],
			"cwd": "${workspaceFolder}"
		},
		{
			"type": "lldb",
			"request": "launch",
			"name": "Debug unit tests in executable 'mysti-client'",
			"cargo": {
				"args": [
					"test",
					"--no-run",
					"--bin=mysti-client",
					"--package=client"
				],
				"filter": {
					"name": "mysti-client",
					"kind": "bin"
				}
			},
			"args": [],
			"cwd": "${workspaceFolder}"
		},
		{
			"type": "lldb",
			"request": "launch",
			"name": "Debug unit tests in library 'common'",
			"cargo": {
				"args": [
					"test",
					"--no-run",
					"--lib",
					"--package=common"
				],
				"filter": {
					"name": "common",
					"kind": "lib"
				}
			},
			"args": [],
			"cwd": "${workspaceFolder}"
		},
		{
			"type": "lldb",
			"request": "launch",
			"name": "Debug executable 'mysti-server'",
			"cargo": {
				"args": [
					"build",
					"--bin=mysti-server",
					"--package=server"
				],
				"filter": {
					"name": "mysti-server",
					"kind": "bin"
				}
			},
			"args": [],
			"cwd": "${workspaceFolder}"
		},
		{
			"type": "lldb",
			"request": "launch",
			"name": "Debug unit tests in executable 'mysti-server'",
			"cargo": {
				"args": [
					"test",
					"--no-run",
					"--bin=mysti-server",
					"--package=server"
				],
				"filter": {
					"name": "mysti-server",
					"kind": "bin"
				}
			},
			"args": [],
			"cwd": "${workspaceFolder}"
		}
	]
}